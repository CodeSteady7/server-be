<% if(alert.message != "") { %>
<script>
 Swal.fire({
    title: "<%= alert.title %>",
    text: "<%= alert.message %>",
    icon: "<%= alert.status %>",
    timer: 2000, // Durasi tampilan pesan dalam milidetik
    showConfirmButton: false, // Hilangkan tombol "OK"
   })
</script>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

<%}%>
