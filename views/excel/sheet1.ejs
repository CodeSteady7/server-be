<div class="size-layout-pages">
  <div class="size-layout-title-pages"
  >
    <div>
      <img src="/images/ic-pertaarungas.png" style="width: 100px" />
    </div>
    <div style="display: flex; flex-direction: column; align-items: center">
      <span style="padding: 3px 0px; font-weight: 700; font-size: 20px;">POWER GENERATION SYSTEM UNIT - 90</span>
      <span style="padding: 3px 0px; font-weight: 700; font-size: 20px;">PG-9001</span>
      <span class="DateSheet" style="padding: 8px 0px 0px 0px; font-weight: 600; font-size: 17px;"> Date : <%= data.queryDate %> </span>
    </div>
    <div>
      <span>PAG-408</span>
    </div>
    <div style="position: absolute;
    right: 9px;
    bottom: 0px;" >
      <h5>SHEET : 1 OF 5</h5>
    </div>
  </div>
  <div class="size-layout-body-pages">
    <table class="styleBorder size-table"  >
      <thead class="thread-pages">
      <tr>
        <th rowspan="3" style="padding: 50px 10px">Hours</th>
        <th colspan="2" rowspan="2">FIELD</th>
        <th
          rowspan="2"
          class="verticalTable "
        >
          VOLTAGE BEFORE </br> TRAFO
        </th>
        <th colspan="3" rowspan="2">VOLTAGE </br> AFTER TRAFO</th>
        <th colspan="3" rowspan="2">LOAD (AMP)</th>
        <th rowspan="3" class="verticalTable widthTable-sheet1 ">POWER FACTOR</th>
        <th colspan="2">LOAD (MW)</th>
        <th colspan="2">M VAR</th>
        <th colspan="9">TEMPERATURE ( Â°C )</th>
        <th colspan="3">GEN TRAFO</th>
        <th rowspan="3" class="verticalTable widthTable-sheet1 ">RECT. TRAFO LIQUID </br> TEMP.</th>
        <th rowspan="3" class="verticalTable widthTable-sheet1 ">KW HOURS</th>
        <th colspan="3" rowspan="2">Visual Check</th>
      </tr>
      <tr>
        <th rowspan="2" class="paddingTable widthTable-sheet1 verticalTable ">METER</th>
        <th rowspan="2" class="paddingTable widthTable-sheet1 verticalTable ">RECORD</th>
        <th rowspan="2" class="paddingTable widthTable-sheet1 verticalTable ">METER</th>
        <th rowspan="2" class="paddingTable widthTable-sheet1 verticalTable ">RECORD</th>
        <th colspan="3">AIR</th>
        <th colspan="6">WINDING</th>
        <th rowspan="2" class="verticalTable widthTable-sheet1 ">LIQUID LEVEL</th>
        <th rowspan="2" class="verticalTable widthTable-sheet1 ">LIQUID TEMP</th>
        <th rowspan="2" class="verticalTable widthTable-sheet1 ">WIND. TEMP</th>
      </tr>
      <tr>
        <th class="paddingTable widthTable-sheet1">V</th>
        <th class="paddingTable widthTable-sheet1">A</th>
        <th class="paddingTable widthTable-sheet1">V</th>
        <th class="paddingTable widthTable-sheet1 verticalTable ">1-2</th>
        <th class="paddingTable widthTable-sheet1 verticalTable ">2-3</th>
        <th class="paddingTable widthTable-sheet1 verticalTable ">3-1</th>
        <th class="paddingTable widthTable-sheet1">1</th>
        <th class="paddingTable widthTable-sheet1">2</th>
        <th class="paddingTable widthTable-sheet1">3</th>
   
        <th class="paddingTable widthTable-sheet1">1</th>
        <th class="paddingTable widthTable-sheet1">2</th>
        <th class="paddingTable widthTable-sheet1">3</th>
        <th class="paddingTable widthTable-sheet1">1</th>
        <th class="paddingTable widthTable-sheet1">2</th>
        <th class="paddingTable widthTable-sheet1">3</th>
        <th class="paddingTable widthTable-sheet1">4</th>
        <th class="paddingTable widthTable-sheet1">5</th>
        <th class="paddingTable widthTable-sheet1">6</th>
        <th class="paddingTable widthTable-sheet1">L/O</th>
        <th class="paddingTable widthTable-sheet1">Temp.</th>
        <th class="paddingTable widthTable-sheet1">Sound</th>
      </tr>
    </thead>
      <tbody style="text-align: center;" class="tbody-pages">

      <!--  -->
      <% for(let i = 0; i < data.temp.length; i++) { %>
       

        <tr>
        <td><%= data.temp[i].clock %></td>
        <td><%= data.temp[i].value.get_field[0]?.valueVField%></td>
        <td><%= data.temp[i].value.get_field[0]?.valueAField%></td>
        <td><%= data.temp[i].value.get_voltBeforeTrafo[0]?.valueV_BT%></td>
        <td><%= data.temp[i].value.get_voltAfterTrafo[0]?.valueVolta1_2%></td>
        <td><%= data.temp[i].value.get_voltAfterTrafo[0]?.valueVolta2_3%></td>
        <td><%= data.temp[i].value.get_voltAfterTrafo[0]?.valueVolta3_1%></td>
        <td><%= data.temp[i].value.get_loadamp[0]?.value1Loadamp%></td>
        <td><%= data.temp[i].value.get_loadamp[0]?.value2Loadamp%></td>
        <td><%= data.temp[i].value.get_loadamp[0]?.value3Loadamp%></td>
        <td><%= data.temp[i].value.get_powerfactor[0]?.valuePowerfactor%></td>
        <td><%= data.temp[i].value.get_loadmw[0]?.valueMeter_loadmw%></td>
        <td><%= data.temp[i].value.get_loadmw[0]?.valueRecord_loadmw%></td>
        <td><%= data.temp[i].value.get_mvar[0]?.valueMeter_mvar%></td>
        <td><%= data.temp[i].value.get_mvar[0]?.valueRecord_mvar%></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td><%= data.temp[i].value.get_genTrafo[0]?.liquid_level%></td>
        <td><%= data.temp[i].value.get_genTrafo[0]?.liquid_temp%></td>
        <td><%= data.temp[i].value.get_genTrafo[0]?.wind_temp%></td>
        <td><%= data.temp[i].value.get_rect_trafo_liquid_temp[0]?.value_tblRect_trafo_liquid_temp%></td>
        <td><%= data.temp[i].value.get_kw_hours[0]?.value_tblkw_hours%></td>
        <td><%= data.temp[i].value.get_visual_check[0]?.l_o%></td>
        <td><%= data.temp[i].value.get_visual_check[0]?.temp%></td>
        <td><%= data.temp[i].value.get_visual_check[0]?.sound%></td>
      </tr>
      <%}%>

    </tbody>
  </table>

  </div>
  <div class="size-layout-footer-pages">
    <div class="signature" style="position:relative">
      <span style="position: absolute; top:10px; left:10px"><%= data.db_signature.range_date01 %></span>
      <div class="signature_user" >
        <h4 style="padding-right: 10px;">Opr.: <span class="signature_user_underline">
       <%= data.db_signature.operation_name_signature01 %>  </span></h4>
        <h4>Supv./Panelman : <span class="signature_user_underline"><%= data.db_signature.suprv_name_signature01 %></span></h4>
      </div>
    </div>
    <div class="signature" style="position:relative">
      <span style="position: absolute; top:10px; left:10px"><%= data.db_signature.range_date02 %></span>
      <div class="signature_user" >
        <h4 style="padding-right: 10px;">Opr.:<span class="signature_user_underline"><%= data.db_signature.operation_name_signature02 %></span> </h4>
        <h4>Supv./Panelman :<span class="signature_user_underline"><%= data.db_signature.suprv_name_signature02 %></span> </h4>
      </div>
    </div>
    <div class="signature" style="position:relative">
      <span style="position: absolute; top:10px; left:10px"><%= data.db_signature.range_date03 %></span>
      <div class="signature_user" >
        <h4 style="padding-right: 10px;">Opr.:<span class="signature_user_underline"> <%= data.db_signature.operation_name_signature03 %></span></h4> <h4>Supv./Panelman :<span class="signature_user_underline"><%= data.db_signature.suprv_name_signature03 %></span> </h4>
      </div>
    </div>
  </div>
</div>
